#!/usr/bin/env perl

use Switch 'Perl6';

use Getopt::Long qw(:config no_ignore_case);

my ($package, $exe, $world);

my $result = GetOptions (
  'module=s' => \$package,
  'all' => \$world,
);

given ($package) {
  when ('tag-rename') {
    $exe = "@CMAKE_BINARY_DIR@/TagRename/tag-rename";
  }
}

print $exe . "\n";
